Bundle-Version:1.0.0.${tstamp}
Bundle-Description: Launches a JavaFX application and provides an instance of javafx.stage.Stage.

	
Export-Package:  \
	com.github.axiopisty.plarpebu.javafx.launcher.api;provide:=true


Require-Capability: \
	compile-only

-runsystempackages: javafx.stage

-buildpath: \
	osgi.enroute.base.api;version=1.0

-testpath: \
	osgi.enroute.junit.wrapper;version=4.12
